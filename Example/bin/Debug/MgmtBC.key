ECHOSUM
SCREEN
************************************************************************************
* Scen 1 - 1000 PL 7.5cm
************************************************************************************
STDIDENT
Scen1
TIMEINT                     10
NUMCYCLE           4
* Turn off ESTAB
ESTAB
STOCKADJ          -1
END
* Read volume limits from external file
OPEN              40
MgmtBCVolume.kcp
ADDFILE           40
CLOSE             40
* Use DBS to capture cut trees, treelist, summary and compute values to MDB
* suppress text file output
TREELIST           0
FVSSTAND           0
STRCLASS
COMPUTE            0
CANOPY = BCANCOV
END
FMIN
SNAGOUT
END
DATABASE
DSNOUT
MgmtBC2.mdb
SUMMARY
COMPUTE
END
* Use DBS to read trees
DATABASE
DSNIN
MgmtBCIn.mdb
TREESQL
SELECT * FROM FVS_TreeInit WHERE STAND_ID = '%STANDID%'
endsql
* Use DBS to read stand info
STANDSQL
SELECT * FROM FVS_StandInit WHERE STAND_ID = '%STANDID%'
endsql
END
PROCESS
