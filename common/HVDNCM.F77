      LOGICAL LNEDNG
      REAL    HVDBA(MXSTND,2),HVDAHT(MXSTND,2),HVDCCF(MXSTND,2),
     &        AXXBA,AXXHT,AXXCCF,HXSIZE
      COMMON /HVDNCM/ HVDBA,HVDAHT,HVDCCF,
     &                AXXBA,AXXHT,AXXCCF,LNEDNG,HXSIZE
